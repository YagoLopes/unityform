    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
